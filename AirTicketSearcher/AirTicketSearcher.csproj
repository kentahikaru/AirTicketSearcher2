  <ItemGroup>
    <PackageReference Include="AsyncAwaitBestPractices" Version="3.0.0" />
    <PackageReference Include="DotnetSpider" Version="4.0.4" />
    <PackageReference Include="htmlagilitypack" Version="1.11.8" />
    <PackageReference Include="NLog" Version="5.0.0-beta11" />
    <PackageReference Include="NLog.Config" Version="4.5.11" />
    <PackageReference Include="NLog.Schema" Version="4.5.11" />
